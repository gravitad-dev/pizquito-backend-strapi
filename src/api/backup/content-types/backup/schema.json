{
  "kind": "collectionType",
  "collectionName": "backups",
  "info": {
    "singularName": "backup",
    "pluralName": "backups",
    "displayName": "Backup"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "filename": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "originalSize": {
      "type": "biginteger",
      "required": true
    },
    "compressedSize": {
      "type": "biginteger"
    },
    "checksum": {
      "type": "string"
    },
    "statusBackup": {
      "type": "enumeration",
      "required": true,
      "default": "pending",
      "enum": [
        "pending",
        "completed",
        "failed",
        "corrupted"
      ]
    },
    "backupType": {
      "type": "enumeration",
      "required": true,
      "default": "manual",
      "enum": [
        "manual",
        "scheduled",
        "other"
      ]
    },
    "description": {
      "type": "text"
    },
    "metadata": {
      "type": "json"
    },
    "filePath": {
      "type": "string",
      "required": true
    }
  }
}
