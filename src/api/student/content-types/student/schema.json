{
  "kind": "collectionType",
  "collectionName": "students",
  "info": {
    "singularName": "student",
    "pluralName": "students",
    "displayName": "Student"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "uid": {
      "type": "uid"
    },
    "profileImage": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "name": {
      "type": "string",
      "required": true
    },
    "lastname": {
      "type": "string",
      "required": true
    },
    "birthdate": {
      "type": "date"
    },
    "nationality": {
      "type": "string"
    },
    "DNI": {
      "type": "string",
      "required": true,
      "unique": true
    },
    "address": {
      "type": "string"
    },
    "country": {
      "type": "string"
    },
    "city": {
      "type": "string"
    },
    "postcode": {
      "type": "string"
    },
    "files": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "notes": {
      "type": "blocks"
    },
    "observations": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::observation.observation",
      "mappedBy": "students"
    },
    "enrollments": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::enrollment.enrollment",
      "mappedBy": "student"
    },
    "guardians": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::guardian.guardian",
      "inversedBy": "students"
    }
  }
}
