{
  "kind": "collectionType",
  "collectionName": "enrollments",
  "info": {
    "singularName": "enrollment",
    "pluralName": "enrollments",
    "displayName": "Enrollment"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "uid": {
      "type": "uid"
    },
    "title": {
      "type": "string"
    },
    "description": {
      "type": "text"
    },
    "school_period": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::school-period.school-period",
      "inversedBy": "enrollments"
    },
    "isActive": {
      "type": "boolean",
      "required": false,
      "default": false
    },
    "files": {
      "type": "media",
      "multiple": true,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "employees": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::employee.employee",
      "mappedBy": "enrollments"
    },
    "guardians": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::guardian.guardian",
      "inversedBy": "enrollments"
    },
    "student": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::student.student",
      "inversedBy": "enrollments"
    },
    "classroom": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::classroom.classroom",
      "inversedBy": "enrollments"
    },
    "services": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::service.service",
      "mappedBy": "enrollments"
    },
    "billingControl": {
      "type": "component",
      "component": "shared.billing-control",
      "repeatable": false
    },
    "invoices": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::invoice.invoice",
      "mappedBy": "enrollment"
    },
    "additionalAmount": {
      "type": "json"
    }
  }
}
